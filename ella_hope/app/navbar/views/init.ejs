
	<div class="container-fluid">
		<div class="navbar-header">
		
		<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#js-navbar">
			<span class="sr-only">Toggle navigation</span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
		</button>
		
		<a class="brand navbar-brand" href="<%== can.route.url({ page: 'dashboard' }); %>">
			<% if (BACKEND_NAME !== undefined || BACKEND_NAME) { %>
				<%= BACKEND_NAME %>
			<% } else { %>
				<%= $.t('project.unknown') %>
			<% } %>
		</a>
	</div>
		<div class="collapse navbar-collapse" id="js-navbar" role="navigation">
			<ul class="nav navbar-nav">
				<li class="dashboard"><%== can.route.link( $.t('navigation.home'), { page: 'dashboard' }, {}, false); %></li>

				<% if (USER.auth_tree.articles) { %>
					<li class="articles"><%== can.route.link( $.t('navigation.articles'), { page: 'articles' }, {}, false); %></li>
				<% } %>

				<% if (1 || USER.auth_tree.galleries) { %>
					<li class="galleries"><%== can.route.link( $.t('navigation.galleries'), { page: 'galleries' }, {}, false); %></li>
				<% } %>

				<% if (USER.auth_tree.articles !== undefined && USER.auth_tree.articles.filmstrip) { %>
					<li class="filmstrips"><%== can.route.link( $.t('navigation.filmstrips'), { page: 'filmstrips' }, {}, false); %></li>
				<% } %>

				<% if (1 || USER.auth_tree.photo) { %>
					<li class="photos"><%== can.route.link( $.t('navigation.photos_videos'), { page: 'photos' }, {}, false); %></li>
				<% } %>

				<% if (USER.auth_tree.contest) { %>
					<li class="contest"><%== can.route.link( $.t('navigation.contests'), { page: 'contest' }); %></li>
				<% } %>

				<% if (USER.auth_tree.quiz) { %>
					<li class="quiz"><%== can.route.link( $.t('navigation.quiz'), { page: 'quiz' }); %></li>
				<% } %>

				<% if (USER.auth_tree.poll) { %>
					<li class="poll"><%== can.route.link( $.t('navigation.polls'), { page: 'poll' }); %></li>
				<% } %>

				<% if (USER.auth_tree.user) { %>
					<li class="user"><%== can.route.link( $.t('navigation.users'), { page: 'user' }); %></li>
				<% } %>

				<% if (USER.auth_tree.admin) { %>
					<li class="admin"><%== can.route.link( $.t('navigation.admin'), { page: 'admin' }); %></li>
				<% } %>

				<li class="dropdown" id="accountmenu">
					<a class="dropdown-toggle" data-toggle="dropdown" href="#"><%= $.t('navigation.account_settings') %><b class="caret"></b></a>
					<ul class="dropdown-menu">
						<li><a href="#"><%= $.t('navigation.profile') %></a></li>
						<li class="divider"></li>
						<li><%== can.route.link( $.t('navigation.logout'), { page: 'user', action: 'logout' }); %></li>
					</ul>
				</li>
			</ul>
		</div><!--/.nav-collapse -->
		<div id="load-state" class="pull-right"></div>
	</div>
