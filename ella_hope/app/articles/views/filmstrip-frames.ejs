<div id="filmstrip-frames" class="control-group clearfix">
	<h3><%= $.t('articles.filmstrip_frames')%></h3>
	
	<span class="btn btn-primary add-filmstrip-frame"><%= $.t('articles.add_filmstrip_frame') %></span>
	
	<% if (!article.id) { %>
		<div class="unsaved-article">
			<label class="label label-warning">
				<%= $.t('articles.save_before_adding')%>
			</label>
		</div>
	<% } %>
		
	<ul style="padding-left: 0">
		<input type="hidden" name="filmstrip-frame-photo-index" id="filmstrip-frame-photo-index" value="">
		<div class="filmstrip-frames-list clearfix" id="filmstrip-frames-list-id">
			<% $.each(filmstripframes || [], function(i, f) { %>
				<%== can.view.render(window.HOPECFG.APP_ROOT + '/articles/views/inline-filmstrip-frame.ejs', {
					item: f,
					index: i
				}) %>
			<% }); %>
		</div>
		<span class="btn btn-primary add-filmstrip-frame"><%= $.t('articles.add_filmstrip_frame') %></span>
	</ul>
</div>
